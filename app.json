{
  "expo": {
    "name": "Sunpeak Coffee",
    "slug": "sunpeak-coffee-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sunpeakcoffee",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f9f506"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sunpeak.coffee"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f9f506"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sunpeak.coffee"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
